{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": "off",
        "indent": ["error", 4],
        "no-else-return": "off",
        "no-useless-escape": "off"
     }
}
